{
  "name": "<%= moduleName %>",
  "version": "1.0.0",
  "description": "",
  "main": "dist/module.js",
  "scripts": {
    "postinstall": "npm run build",
    "precommit": "npm test && npm run build",
    "build": "rimraf dist && webpack --no-minimize && webpack",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report text node_modules/mocha/bin/_mocha",
    "coveralls": "npm run coverage && coveralls < coverage/lcov.info",
    "test": "npm run test-server && npm run test-browser",
    "test-server": "mocha --recursive --compilers js:babel-core/register --reporter nyan",
    "test-browser": "karma start --single-run",
    "tdd": "npm test -- --watch",
    "tdd-browser": "karma start"
  },
  "repository": "<%= username %>/<%= moduleName %>",
  "keywords": [
  ],
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "babel": "^5.8.3",
    "babel-core": "^5.8.3",
    "babel-loader": "^5.3.3",
    "chai": "^3.2.0",
    "coveralls": "^2.11.4",
    "eslint": "^1.4.1",
    "husky": "^0.10.1",
    "isparta": "^3.5.1",
    "karma": "^0.13.14",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-spec-reporter": "0.0.18",
    "karma-webpack": "^1.5.0",
    "mocha": "^2.3.3",
    "phantomjs-polyfill": "0.0.1",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.2"
  },
  "pre-commit": [
    "test",
    "test:browser",
    "build"
  ],
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "license": "MIT",
  "dependencies": {}
}
